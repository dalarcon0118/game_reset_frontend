{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node start-dev.js",
    "start:dev": "node start-dev.js",
    "start:render": "node start-dev.js",
    "dev": "npm run start:dev",
    "render": "npm run start:render",
    "build:web": "EXPO_ROUTER_APP_ROOT=app expo export --platform web",
    "build:apk": "eas build --profile preview --platform android",
    "build:prod": "eas build --profile production --platform android",
    "lint": "expo lint",
    "test": "jest",
    "test:all": "jest tests",
    "test:feature": "node -e \"const feature = process.env.npm_config_feature; if (!feature) { console.error('Please specify a feature: npm run test:feature --feature=draws'); process.exit(1); } require('child_process').spawnSync('jest', ['tests/features/admin/' + feature], { stdio: 'inherit', shell: true });\"",
    "test:watch": "jest --watch",
    "postinstall": "mkdir -p ../node_modules/expo-router && cp patches/expo-router-fix/_ctx.android.js ../node_modules/expo-router/_ctx.android.js || true",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@eva-design/eva": "^2.2.0",
    "@expo/vector-icons": "~14.0.4",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@ui-kitten/components": "^5.3.1",
    "@ui-kitten/eva-icons": "^5.3.1",
    "event-source-polyfill": "^1.0.31",
    "expo": "~52.0.47",
    "expo-blur": "~14.0.3",
    "expo-camera": "~16.0.18",
    "expo-constants": "~17.0.8",
    "expo-crypto": "~14.0.2",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.21",
    "expo-secure-store": "~14.0.1",
    "expo-sharing": "~13.0.1",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "^0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.2",
    "jscodeshift": "^17.3.0",
    "json-logic-js": "^2.0.5",
    "lucide-react-native": "^0.475.0",
    "metro-transform-plugins": "^0.83.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.56.4",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-view-shot": "~4.0.3",
    "react-native-web": "^0.19.13",
    "react-native-webview": "13.12.5",
    "web-streams-polyfill": "^4.2.0",
    "zustand": "^5.0.5"
  },
  "overrides": {
    "react-native-svg": "15.8.0",
    "react-test-renderer": "18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@cucumber/cucumber": "^12.6.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/json-logic-js": "^2.0.8",
    "@types/react": "~18.3.12",
    "axios-mock-adapter": "^2.1.0",
    "babel-jest": "^29.7.0",
    "babel-preset-expo": "~12.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "hermes-parser": "^0.23.1",
    "jest": "~29.7.0",
    "jest-cucumber": "^4.5.0",
    "jest-expo": "~52.0.6",
    "jest-fetch-mock": "^3.0.3",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.3"
  }
}
