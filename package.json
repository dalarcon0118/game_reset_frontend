{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "EXPO_ROUTER_APP_ROOT=app expo start",
    "start:dev": "EXPO_PUBLIC_APP_ENV=development EXPO_NO_TELEMETRY=1 EXPO_ROUTER_APP_ROOT=app expo start",
    "start:render": "EXPO_PUBLIC_APP_ENV=production EXPO_NO_TELEMETRY=1 EXPO_ROUTER_APP_ROOT=app expo start",
    "dev": "npm run start:dev",
    "render": "npm run start:render",
    "build:web": "EXPO_ROUTER_APP_ROOT=app expo export --platform web",
    "build:apk": "eas build --profile preview --platform android",
    "build:prod": "eas build --profile production --platform android",
    "lint": "expo lint",
    "postinstall": "cp patches/expo-router-fix/_ctx.android.js node_modules/expo-router/_ctx.android.js"
  },
  "dependencies": {
    "@eva-design/eva": "^2.2.0",
    "@expo/vector-icons": "~14.0.4",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@ui-kitten/components": "^5.3.1",
    "@ui-kitten/eva-icons": "^5.3.1",
    "expo": "~52.0.47",
    "expo-blur": "~14.0.3",
    "expo-camera": "~16.0.18",
    "expo-constants": "^17.0.5",
    "expo-crypto": "^15.0.8",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "^7.0.5",
    "expo-router": "~4.0.21",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "^0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.2",
    "json-logic-js": "^2.0.5",
    "lucide-react-native": "^0.475.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.56.4",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.19.13",
    "react-native-webview": "13.12.5",
    "zustand": "^5.0.5"
  },
  "overrides": {
    "react-native-svg": "15.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/json-logic-js": "^2.0.8",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.72.8",
    "babel-preset-expo": "~12.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "hermes-parser": "^0.23.1",
    "typescript": "^5.3.3"
  }
}
