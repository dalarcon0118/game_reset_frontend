{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "../frontend_",
    "strict": true,
    "paths": {
      "@/*": [
        "../frontend_/*"
      ],
      "@shared/*": [
        "../frontend_/shared/*"
      ],
      "@features/*": [
        "../frontend_/features/*"
      ],
      "@core/*": [
        "../frontend_/shared/core/*"
      ]
    }
  },
  "include": [
    "../frontend_/**/*.ts",
    "../frontend_/**/*.tsx",
    "../frontend_/.expo/types/**/*.ts",
    "../frontend_/expo-env.d.ts",
    "../frontend_/nativewind-env.d.ts"
, "../frontend_/features/banker/common/header"  ],
  "exclude": [
    "../frontend_/**/legacy/**"
  ]
}
