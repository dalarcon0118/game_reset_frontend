{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "resourceClass": "large",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development",
        "EXPO_PUBLIC_APP_ENV": "development",
        "GRADLE_OPTS": "-Dorg.gradle.jvmargs=\"-Xmx16g -Xss4m -XX:MaxMetaspaceSize=2g -XX:+UseG1GC\"",
        "NODE_OPTIONS": "--max-old-space-size=8192"
      }
    },
    "preview": {
      "resourceClass": "large",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_APP_ENV": "production",
        "GRADLE_OPTS": "-Dorg.gradle.jvmargs=\"-Xmx16g -Xss4m -XX:MaxMetaspaceSize=2g -XX:+UseG1GC\"",
        "NODE_OPTIONS": "--max-old-space-size=8192"
      }
    },
    "production": {
      "resourceClass": "large",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_APP_ENV": "production",
        "GRADLE_OPTS": "-Dorg.gradle.jvmargs=\"-Xmx16g -Xss4m -XX:MaxMetaspaceSize=2g -XX:+UseG1GC\"",
        "NODE_OPTIONS": "--max-old-space-size=8192"
      }
    }
  },
  "submit": {
    "production": {}
  }
}